                android:layout_height="wrap_content"
                android:layout_margin="@dimen/_10sdp"
                android:background="@color/gray_bg"
                android:textSize="@dimen/_12ssp"
                app:layout_constraintStart_toEndOf="@id/crdOrdered1"
