                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_55sdp"
                android:fontFamily="@font/roboto_medium"
                android:text="@string/delivered"
                android:textColor="@color/black"
                android:textSize="@dimen/_12ssp"
                android:visibility="gone"
                app:layout_constraintStart_toEndOf="@id/crdDelivered1"
                app:layout_constraintTop_toBottomOf="@id/txtOrderedText1" />

            <View
                android:id="@+id/viewDelivered1"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1sdp"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                android:background="@color/gray_editext_bg"
                app:layout_constraintTop_toBottomOf="@id/crdDelivered1" />

            <View
