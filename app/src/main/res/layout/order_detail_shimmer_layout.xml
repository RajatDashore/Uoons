                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginEnd="@dimen/_5sdp"
                android:background="@color/gray_bg"
                android:textSize="@dimen/text_size14"
                app:layout_constraintEnd_toEndOf="parent"
