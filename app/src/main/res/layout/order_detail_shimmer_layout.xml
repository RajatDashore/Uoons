                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginEnd="@dimen/_10sdp"
                android:background="@color/gray_bg"
                android:textSize="@dimen/_12ssp"
                app:layout_constraintEnd_toStartOf="@+id/ivProductImage1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/txvProductName1" />

            <View
                android:id="@+id/viewOffer1"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1sdp"
                android:layout_margin="@dimen/_10sdp"
                android:background="@color/gray_editext_bg"
                app:layout_constraintTop_toBottomOf="@id/ivProductImage1" />

            <androidx.cardview.widget.CardView
                android:id="@+id/crdOrdered1"
